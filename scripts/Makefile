# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  mossCore_config = debug
endif
ifeq ($(config),release)
  mossCore_config = release
endif

PROJECTS := mossCore

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

mossCore:
ifneq (,$(mossCore_config))
	@echo "==== Building mossCore ($(mossCore_config)) ===="
	@${MAKE} --no-print-directory -C . -f mossCore.make config=$(mossCore_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f mossCore.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   mossCore"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"